 <div class="modal fade" id="{{ modal_id }}" tabindex="-1" role="dialog" aria-labelledby="showAllLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title" id="showAllLabel">All events for {{ modal_title_attendee }}</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            <div class="list-group">
                {% for registration in modal_registrations %}
                    <li class="list-group-item list-group-item-action">{{ registration.events.eventType.slug }}</li>
                {% endfor %}
            </div>
        </div>
        </div>
    </div>
</div>
